@article{bryden_how_2018,
	title = {How humans transmit language: {Horizontal} transmission matches word frequencies among peers on {Twitter}},
	volume = {15},
	issn = {1742-5689, 1742-5662},
	shorttitle = {How humans transmit language},
	url = {http://rsif.royalsocietypublishing.org/lookup/doi/10.1098/rsif.2017.0738},
	doi = {10.1098/rsif.2017.0738},
	abstract = {Language transmission, the passing on of language features such as words between
people, is the process of inheritance that underlies linguistic evolution. To understand
how language transmission works we need a mechanistic understanding based on
empirical evidence of lasting change of language usage. Here, we analysed 200
million online conversations to investigate transmission between individuals. We
 nd that frequency of word usage is inherited over conversations, not just the binary
presence or absence of a word in a person's lexicon. We propose a mechanism for
transmission whereby for each word someone encounters there is a chance they will
use it more often. Using this mechanism, we measure that for one word in around
every hundred someone encounters, they will use that word more frequently. Since
more commonly used words are encountered more often, this means that it is the
frequencies of words which are copied. Beyond this, our measurements indicate that
this per-encounter mechanism is neutral and applies without any further distinction
as to whether a word encountered in a conversation is commonly used or not. An
important consequence of this is that frequencies of many words can be used in
concert to observe and measure language transmission, and our results con rm this.
These results indicate that our mechanism for transmission can be used to study
language patterns and evolution within populations.},
	language = {English},
	number = {139},
	urldate = {2018-03-29},
	journal = {Preprint, Journal of The Royal Society Interface},
	author = {Bryden, John and Wright, Shaun P. and Jansen, Vincent A. A.},
	month = feb,
	year = {2018},
	pages = {1--14},
	file = {Bryden et al. - 2018 - How humans transmit language horizontal transmiss.pdf:C\:\\Users\\josh8\\Zotero\\storage\\8M3DPXGA\\Bryden et al. - 2018 - How humans transmit language horizontal transmiss.pdf:application/pdf}
}

@article{schnoebelen_you_2012,
	title = {Do {You} {Smile} with {Your} {Nose}? {Stylistic} {Variation} in {Twitter} {Emoticons}},
