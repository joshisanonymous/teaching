	year = {1998},
	keywords = {*Acadian French (00073), *Age Differences (01150), *Language Use (44610), *Social Factors (79910)},
	pages = {161--188},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\Z2QXNIV8\\3.html:text/html}
}

