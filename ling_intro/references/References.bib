@article{rottet_language_1996,
	title = {Language {Change} and {Language} {Death}: {Some} {Changes} in the {Pronominal} {System} of {Declining} {Cajun} {French}},
	volume = {11},
	issn = {1157-5069},
	shorttitle = {Language {Change} and {Language} {Death}},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/58331467/13F5F84C6944BC22F98/92?accountid=14437},
	abstract = {The decline of Louisiana Cajun French (LCF), due to both shift toward the use of American English, \& changes to the internal structure of LCF, is discussed. Causes \& manifestations of LCF shift toward American English since 1930 are noted, \& discussion of internal changes focuses on LCF pronominal change over three generations. This change is analyzed using transcriptions of free conversations \& oral translations by LCF speakers (N = 76) Ss were divided into three groups aged under 30, 30-54, \& 55+. Two kinds of changes are found in first person singular constructions: loss of emphatic or contrastive effect in the disjunctive pronoun moi 'me, myself', sometimes used as the subject replacing je 'I'; \& increasing occurrence of null subjects. Second person structures showed an increase in use of tu 'you' (informal singular) in polite contexts, a decrease in vous 'you' (polite singular) \& vous-autres 'you' (informal plural), \& elimination of most -ez verb endings. In the various 3rd person plurals, the pronoun ils 'they' \& virtually all subject-verb agreement were eliminated, younger speakers displayed a preference for using ca to designate 'they', \& the use of eusse \& eux-autres for 'they' was also in frequent use. The partitive pronoun en 'some of it/them' became a postverbal free morpheme zen. Some innovations appear with the losses, but the data confirm the decline of LCF. 34 References. E. Taylor},
	language = {English},
	number = {Apr},
