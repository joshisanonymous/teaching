@phdthesis{kazerounian_laminar_2012,
	title = {A laminar cortical model of conscious speech perception: {Phonemic} restoration and speech category learning},
	shorttitle = {A laminar cortical model of conscious speech perception},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/1323344786/abstract/13F4B32A0B6D6605BC/9?accountid=14437},
	abstract = {How do the laminar circuits of neocortex learn categories that can support conscious percepts of speech and language? How do learned speech categories become selectively tuned to different temporal sequences of speech items that are stored in short-term working memory in real time? How does the brain use resonant feedback between working memories and learned categories to restore information that is occluded by noise using the context of a word or sentence? A model is developed to simulate how multiple laminar cortical processing stages interact to support a conscious speech percept. In particular, acoustic features are unitized into acoustic items. These items activate representations in an item-and-order, or competitive queuing, sequential short-term working memory. The sequence of stored working memory items interacts reciprocally with unitized representations of item sequences, also called list categories or chunks, in a multiple-scale categorization network, called a masking field, that is capable of weighing the evidence for groupings of variable-length sequences of items as they are stored in the working memory through time. List chunks represent the most predictive item groupings at any time. These bottom-up and top-down interactions between auditory features, working memory, and list chunks generate a resonant wave of activation whose attended features embody consciously heard percepts, notably the completed percepts that can form even when acoustic information may be missing or occluded by noise. This occurs in the auditory illusion known as phonemic restoration, even if the disambiguating speech context occurs after the occluding noise. This thesis provides the first explanation and simulation of how phonemic restoration arises in a laminar cortical hierarchy. It also develops a masking field that learns to respond robustly to input patterns from working memory as they unfold in time. Notably, for a given number of input items, all possible ordered sets of these items up to a fixed length can be learned. Both unsupervised and supervised learning simulations are provided. Supervised learning does not require as many list chunks to learn arbitrary sequences. Copies of dissertations may be obtained by addressing your request to ProQuest, 789 E. Eisenhower Parkway, P.O. Box 1346, Ann Arbor, MI 48106-1346. Telephone 1-800-521-3042; e-mail: disspub@umi.com},
	language = {English},
	urldate = {2013-07-16},
	author = {Kazerounian, Sohrob},
	year = {2012},
	keywords = {*Acoustics (00200), *Context (15250), *Learning (45750), *Masking (51450), *Noise (58100), *Phonemics (64670), *Resonants (72985), *Short Term Memory (78150), *Time (89850)},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\AUXL4MN7\\9.html:text/html}
}

@article{srinivasan_schema-based_2005,
	title = {A schema-based model for phonemic restoration},
	volume = {45},
	issn = {0167-6393},
	url = {http://www.sciencedirect.com/science/article/pii/S0167639304000986},
	doi = {10.1016/j.specom.2004.09.002},
	abstract = {Phonemic restoration is the perceptual synthesis of phonemes when masked by appropriate replacement sounds by utilizing linguistic context. Current models attempting to accomplish acoustic restoration of phonemes, however, use only temporal continuity and produce poor restoration of unvoiced phonemes, and are also limited in their ability to restore voiced phonemes. We present a schema-based model for phonemic restoration. The model employs a missing data speech recognition system to decode speech based on intact portions and activates word templates corresponding to the words containing the masked phonemes. An activated template is dynamically time warped to the noisy word and is then used to restore the speech frames corresponding to the masked phoneme, thereby synthesizing it. The model is able to restore both voiced and unvoiced phonemes with a high degree of naturalness. Systematic testing shows that this model outperforms a Kalman-filter based model.},
	number = {1},
	urldate = {2013-07-16},
	journal = {Speech Communication},
	author = {Srinivasan, Soundararajan and Wang, DeLiang},
	month = jan,
	year = {2005},
	keywords = {Computational auditory scene analysis, Dynamic time warping, Missing data ASR, Prediction, Speech schemas, Top-down model, phonemic restoration},
	pages = {63--87},
	file = {ScienceDirect Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\BKRM7Y3P\\S0167639304000986.html:text/html}
}

@phdthesis{spahr_perception_2005,
	title = {The {Perception} of {Speech}, {Voice} and {Music} by {Patients} {Fit} with {Different} {Cochlear} {Implant} {Systems}},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/85622862/abstract/13F4B255FBF109BA323/6?accountid=14437},
	abstract = {The aim of this dissertation was to assess the perception of speech, voice and music in three groups of patients fit with cochlear implants. The patients in one group used the Advanced Bionics CII Bionic Ear system and the Hi-Resolution speech coding strategy. The patients in a second group used the Cochlear Corporation Esprit 3G processor and Advanced Combination Encoder speech coding strategy. The patients in a third group used the Med El Combi40+ system and CIS speech coding strategy. At issue was whether the different implant systems provided different levels of speech, voice or music recognition for patients. The results of Experiment 1 indicated that (i) patients can achieve high levels of speech understanding in quiet backgrounds, (ii) patients experience difficulty when speech is presented in noise backgrounds or at low levels, and (iii) patients achieved higher levels of speech understanding when speech was cued by information in the amplitude envelope than when speech was cued by information in the spectral domain. Differences in group mean performance were observed on tests of synthetic vowel recognition, the recognition of speech in noise and the recognition of speech at low levels. The pattern of results suggested that between-device differences in input dynamic range were responsible for most of time between-device differences in performance. In Experiment 2 the input dynamic range was increased from 30 to 40 dB in the 3C speech processor. The outcome was a significant interaction between device setting and performance in difficult listening situations. Increasing input dynamic range improved performance at low levels but degraded performance in noise. In Experiment 3 the input dynamic range was varied over the range 30-60 dB in the CII Bionic Ear system. The outcome was a significant increase in speech understanding at low levels and an increase in speech understanding in noise. The different results for patients fit with time 3G and CII system were likely due to differences in the compression functions used to expand input dynamic range in the two devices.},
	language = {English},
	urldate = {2013-07-16},
	author = {Spahr, Anthony J.},
	year = {2005},
	keywords = {*Auditory Perception (05800), *Cochlear Implants (12642), *Frequency (Acoustics) (26100), *Intensity (Acoustics) (36650), *Music (55800), *Noise (58100), *Sound Identification (80450), *Speech Perception (82700)},
