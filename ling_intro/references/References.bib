	year = {1996},
	keywords = {Louisiana Cajun French decline, conversations/oral translations, native speakers, pronominal focus, shift/internal changes},
	pages = {117--151},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\M98LZPBK\\92.html:text/html}
}

