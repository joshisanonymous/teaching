@phdthesis{xu_consonant_2005,
	title = {Consonant {Perturbation} and {Tone} {Restoration} in {Mandarin}},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/docview/85615401?accountid=14437},
	abstract = {This thesis is meant to further our understanding of an important variable in speech communication, fundamental frequency (F0). Towards this goal, I investigated F0 perturbation by consonants and lexical tone restoration in continuous speech of Mandarin Chinese. Mandarin is atone language and its lexical tones have relatively stable F0 patterns, which offer good references for studying various effects on surface F0, contours. F0 perturbation by consonants is an involuntary F0 variation during speech production. The clarification of involuntary effects on pitch movements can provide a baseline for studying the communicative factors and shed light on underlying mechanisms of articulatory constraints. By analyzing the main effect of consonant perturbation on F0, I found that Mandarin voiceless consonants give rise to higher F0 onsets in the following vowel as opposed to nasal consonants, with aspirated stops associated with lower values than their unaspirated cognates. By analyzing the interaction of consonant perturbation with lexical tone and tonal coarticulation on F0 I proposed that this perturbation effect is attributable to myoelastic factors, as well as aerodynamic factors. The analyses of interaction also show that the effect of consonant perturbation is local and it does not change the underlying pitch targets of lexical tones or the performance of tonal coarticulation. Rather, it is superimposed onto the F0 contours resulting from known contextual tonal variations without altering the general course of the F0 movements. The above findings of diverse F0 variations in speech production may raise the question as to how those F0 variations operate in speech perception. Based on the broad distribution of tonal information in the speech signal, I hypothesized that F0, may function as a robust acoustic cue in auditory compensation and tested this hypothesis with experiments on phonemic restoration. The important function of F0 in auditory compensation is confirmed by the evidence that contextual tonal variations resulting from carryover effects can lead to restoration of lexical tones. This finding suggests the importance of taking advantage of the robustness of the F0 cue in speech technology so that ASR systems can be applied to real-life environments.},
	language = {English},
	urldate = {2013-02-25},
	author = {Xu, Xueqing},
	year = {2005},
	keywords = {*Coarticulation (Context Effects) (12628), *Consonants (14900), *Fundamental Frequency (26600), *Mandarin (50900), *Phonology Phonetics Relationship (65260), *Tone (90300)},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\FVBHLAR4\\85615401.html:text/html}
}

