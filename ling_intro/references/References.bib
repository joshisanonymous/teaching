	keywords = {Condensed Matter - Statistical Mechanics, Condensed Matter - Disordered Systems and Neural Networks},
	pages = {1--14},
	file = {arXiv\:cond-mat/0209450 PDF:C\:\\Users\\josh8\\Zotero\\storage\\TYY3E6QM\\Newman - 2003 - Mixing patterns in networks.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\IKVCISJY\\0209450.html:text/html}
}

@article{newman_modularity_2006,
