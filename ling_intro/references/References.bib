	keywords = {Registers, indexicality, Honorifics, Language ideology, Political economy of language, Sociolinguistic variation},
	pages = {193--229},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\josh8\\Zotero\\storage\\7A8F6FI4\\Silverstein - 2003 - Indexical order and the dialectics of sociolinguis.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\5TH983AL\\S0271530903000132.html:text/html}
